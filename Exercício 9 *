using System;

class MainClass {
  static void Main (string[] args) {
    float media = 0,nota1 = 0,nota2 = 0,examesemest = 0;
    Console.WriteLine("Digite nota1: ");
    nota1 = float.Parse(Console.ReadLine());
    Console.WriteLine("Digite nota2: ");
    nota2 = float.Parse(Console.ReadLine());
    Console.WriteLine("Digite sua nota no examesemest: ");
    examesemest = float.Parse(Console.ReadLine());
   
    media = ((nota1+nota2+examesemest)/3);
    media = (float)(Math.Round((float)media, 2));
    Console.WriteLine("Sua média final é: {0}", media);

    if (media >= 5) {
      Console.WriteLine("Aprovado!");
    }else{
      Console.WriteLine("Reprovado!");
    }

  }
}
