using System;

class MainClass {
  static void Main (string[] args) {
    int hora;
    Console.WriteLine ("Entre com a hora desejada: ");
    hora = int.Parse(Console.ReadLine());

    if (hora >= 01 & hora <= 12) {
      Console.WriteLine("Bom dia!");
    }
    if (hora > 12 & hora < 18) {
      Console.WriteLine("Boa tarde!");
    }
    if (hora >= 18 & hora <= 23) {
      Console.WriteLine("Boa Noite!");
    }

    }
  }
